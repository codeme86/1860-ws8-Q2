CHIP DisplayCounter201890716 {
    IN inc, reset;
    OUT a, b, c, d, e, f, g;

    PARTS:
    Inc16(in=outBus, out=incOut);
    Mux16(a=incOut, b=false, sel=reset, out=next);
    Register(in=next, load=inc, out=outBus);
    Decoder201890716(
        A=outBus[0],
        B=outBus[1],
        C=outBus[2],
        D=outBus[3],
        a=a,
        b=b,
        c=c,
        d=d,
        e=e,
        f=f,
        g=g
    );
}